{
    "name": "chriscupas/analytics",
    "description": "Analytics for the Laravel framework.",
    "keywords": ["laravel", "analytics", "middleware", "package"],
    "license": "MIT",
    "authors": [
        {
            "name": "chriscupas",
            "email": "chriscupas@yahoo.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "ext-intl": "*",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/collections": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/view": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "jenssegers/agent": "^2.6",
        "jaybizzle/crawler-detect": "^1.2",
        "doctrine/dbal": "^3.2 || ^4.0",
        "mobiledetect/mobiledetectlib": "^2.7.6"
    },
    "autoload": {
        "psr-4": {
            "AndreasElia\\Analytics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AndreasElia\\Analytics\\Tests\\": "tests/",
            "AndreasElia\\Analytics\\Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AndreasElia\\Analytics\\AnalyticsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^6.0 || ^7.0 || ^8.0 || ^9.0",
        "phpunit/phpunit": "^9.5 || ^10.0 || ^11.0",
        "laravel/pint": "^1.24",
        "friendsofphp/php-cs-fixer": "^3.86"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
